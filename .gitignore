
[core]
    excludesfile = ~/.gitignore

# Mac OS X Finder
.DS_Store

# XCode
*.mode1
*.mode1v3
*.mode2v3
*.perspective
*.perspectivev3
*.pbxuser

# Xcode 4 and later
project.xcworkspace/
xcuserdata/
*.xcworkspacesdata
*.xcuserstate

# Generated files
VersionX-revision.h
*.o
*.pyc
*.hi

# Build products
build/
*.[oa]

# Other source repository archive directories (protects when importing)
.hg
.svn
CVS
.cvs
.cvswrappers
.cvs*

# Automatic backup files
*~.nib
*.swp
*~
*(Autosaved).rtfd/
Backup[ ]of[ ]*.pages/
Backup[ ]of[ ]*.key/
Backup[ ]of[ ]*.numbers/

# Python modules
MANIFEST
dist/
build/

# generated sounds
Sounds/Outputs/*
C++/FFTConvolver/*
